{
  "name": "@microdrop/application",
  "version": "1.1.23",
  "description": "Top Level Application for Microdrop",
  "main": "index.js",
  "bin": {
    "microdrop-3": "bin/microdrop-3"
  },
  "default-enabled": [
    "device-ui-plugin",
    "state-saver"
  ],
  "scripts": {
    "test": "node _test.js",
    "start": "./node_modules/.bin/electron .",
    "clear:appdata": "./node_modules/.bin/electron clear-data.js",
    "build": "npm run build:phosphor && npm run build:ui",
    "build:phosphor": " cd ./ui/src && ../../node_modules/.bin/webpack --config phosphor.config.js",
    "build:ui": "cd ./ui/src && ../../node_modules/.bin/webpack --config plugin-manager.config.js"
  },
  "author": "Sci-Bots",
  "license": "BSD-2-Clause",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@microdrop/buildutils": "^1.1.0",
    "@microdrop/device-controller": "^1.1.23",
    "@micropede/broker": "^0.0.11",
    "@micropede/client": "^0.0.11",
    "ajv": "^6.0.0-beta.2",
    "argparse": "^1.0.9",
    "backbone": "^1.3.3",
    "crossroads": "^0.12.2",
    "eventemitter2": "^5.0.0",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "handlebars": "^4.0.11",
    "ioredis": "^3.2.2",
    "level-js": "^2.2.4",
    "lodash": "^4.17.4",
    "mongodb": "^3.0.1",
    "mosca": "^2.7.0",
    "mqtt": "^2.14.0",
    "mqtt-router": "^0.5.0",
    "msgpack-lite": "^0.1.26",
    "pandas-helpers": "^1.0.0",
    "three": "^0.89.0",
    "uuid": "^3.1.0",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "electron": "^1.7.10",
    "electron-mocha": "^5.0.0",
    "webpack": "^3.8.1"
  },
  "build": {
    "appId": "com.sci-bots.microdrop",
    "portable": {
      "requestExecutionLevel": "user"
    }
  }
}
